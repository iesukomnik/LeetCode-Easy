char * longestCommonPrefix(char ** strs, int strsSize){
    //*strs = &strs[0]
    //strs = {"","",....};
    
    int minstr=strlen(strs[0]);
    
    //find lenght of shortest string
    for(int i=0;i<strsSize;i++){
        if(strlen(strs[i])<minstr)
            minstr=strlen(strs[i]);
    }
    
    //I don't know why minstr has to add 1
    char* ans;
    ans = (char *)malloc(minstr+1);
    memset(ans,'\0',minstr+1);
        
    for(int i=0;i<minstr;i++){
        for(int j=0;j<strsSize-1;j++){
            if(strs[j][i] != strs[j+1][i])
                return ans;
        }
        ans[i]=strs[0][i];
    }
    
    return ans;
}
